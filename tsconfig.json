{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "experimentalDecorators": true,
    "jsx": "react",
    "noEmitOnError": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "lib",
    "lib-amd",
    "plugin-files"
  ]
}
